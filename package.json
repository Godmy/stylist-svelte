{
  "name": "stylist-svelte",
  "version": "0.7.0",
  "description": "A comprehensive Svelte 5 UI component library with Tailwind CSS",
  "type": "module",
  "author": "Godmy & AI-Team",
  "license": "MIT",
  "packageManager": "yarn@4.5.1",
  "keywords": [
    "svelte",
    "svelte5",
    "ui",
    "components",
    "tailwind",
    "design-system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Godmy/stylist-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/Godmy/stylist-svelte/issues"
  },
  "homepage": "https://github.com/Godmy/stylist-svelte#readme",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-package && publint",
    "preview": "vite preview",
    "package": "svelte-package",
    "prepublishOnly": "npm run build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:unit": "vitest run",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "clean": "rm -rf dist .svelte-kit node_modules/.vite"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./components/atoms": {
      "types": "./dist/components/atoms/index.d.ts",
      "svelte": "./dist/components/atoms/index.js"
    },
    "./components/molecules": {
      "types": "./dist/components/molecules/index.d.ts",
      "svelte": "./dist/components/molecules/index.js"
    },
    "./components/organisms": {
      "types": "./dist/components/organisms/index.d.ts",
      "svelte": "./dist/components/organisms/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    },
    "./playground": {
      "types": "./dist/playground/index.d.ts",
      "svelte": "./dist/playground/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/svelte": "^5.2.8",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.15",
    "svelte": "^5.39.5",
    "svelte-check": "^4.3.2",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "lucide-svelte": "^0.468.0",
    "marked": "^17.0.0",
    "shiki": "^3.15.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
